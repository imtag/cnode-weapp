@font-face {
  font-family: 'iconfont';
  src: url('data:application/x-font-woff;charset=utf-8;base64,');
}
.icon {
  display: inline-block;
  vertical-align: middle;
  font-family: 'iconfont' !important;
  font-style: normal;
  font-size: 40rpx;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-loading {
  transform-origin: center;
  animation: rotate .8s linear infinite;
}
@keyframes rotate {
  from {
    transform: rotate(0deg)
  } 
  to {
    transform: rotate(360deg)
  }
}

.icon-add:before {
  content: "\e900";
}
.icon-arrow-up:before {
  content: "\e901";
}
.icon-comment:before {
  content: "\e902";
}
.icon-follow-active:before {
  content: "\e903";
}
.icon-follow:before {
  content: "\e904";
  color: #666;
}
.icon-like-active:before {
  content: "\e905";
  color: #666;
}
.icon-like:before {
  content: "\e906";
  color: #666;
}
.icon-loading:before {
  content: "\e907";
}
.icon-nodata:before {
  content: "\e908";
  color: #8a96a3;
}
.icon-nonetwork:before {
  content: "\e909";
}